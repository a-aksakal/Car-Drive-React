{
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "ecmaFeatures": {}
    },
    "rules": {
        "no-var": 2,
        "no-useless-return": 2,
        "no-unused-vars": 2,
        "no-unused-labels": 2,
        "vars-on-top": 2,
        "prefer-const": 2,
        "no-multi-spaces": 2,
        "no-multiple-empty-lines":2
    },
    "env": {}
}